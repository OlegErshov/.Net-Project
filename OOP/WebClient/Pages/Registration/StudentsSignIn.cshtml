@page
@model WebClient.Pages.Registration.StudentsSignInModel
@{
}

<div class="row">
    <label>МЫ СКУЧАЛИ</label>
</div>

<form method="post">
    <div class="row">
        <div class="card-deck">

            <div class="card-header">
                <label>Regictration</label>
            </div>

            <div class="card-body">
